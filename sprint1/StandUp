CREATE TABLE User (
    UserID INT AUTO_INCREMENT PRIMARY KEY,
    UserName VARCHAR(50) NOT NULL,
    UserPassword VARCHAR(255) NOT NULL,
    Email VARCHAR(100) NOT NULL UNIQUE,
    Gender VARCHAR(10),
    Status INT,
    ActiveCode INT,
    `Character` INT  -- Enclose "Character" in backticks to avoid conflict with SQL keywords
); this is our database for user 


